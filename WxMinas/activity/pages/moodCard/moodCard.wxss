/* pages/moodCard/moodCard.wxss */
.container {
    padding: 20rpx 0rpx;
    background-color: #FBFBFB;
}
.moodCard {
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    width: 100%;
    margin-bottom: 20rpx;
    /* background: lightblue; */
}

.finalOperate {
    width: 100%;
    height: 260rpx; 
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
}
.finalOperate button {
    width: 80%;
    height: 95rpx;
    margin: 20rpx;
    line-height: 90rpx;
}
.saveMoodCard {
    background-color: #19AD19;
    color: #ffffff;
}
.inviteToPlay {
    background-color: #ffffff;
}
