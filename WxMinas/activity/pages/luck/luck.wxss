/* pages/luck/luck.wxss */
.canvas-model {
    position: fixed;
    top: -5000px;
}

.name-input {
    margin: 20rpx 0;
    height: 70rpx;
}

.invite-dialog {
    display: flex;
    align-items: center;
    justify-content: center;
    width: 100%;
    height: 100%;
    box-sizing: border-box;
    position: fixed;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    z-index: 100;
    margin: auto;
    background-color: rgba(0, 0, 0, .8);
}

.invite-dialog .dialog-main {
    position: absolute;
    left: 0;
    right: 0;
    bottom: 0;
    top: 0;
    z-index: 1;
    margin: auto;
    padding: 100rpx 0 200rpx;
    box-sizing: border-box;
    width: 100%;
    height: 100%;
    display: flex;
    align-items: center;
    justify-content: center;
}

.invite-dialog .close-btn {
    position: absolute;
    right: -88rpx;
    top: 0;
    z-index: 10;
    width: 88rpx;
    height: 88rpx;
    background: transparent url(data:image/png;base64,) center top no-repeat;
    background-size: 48rpx;
}

.card-wrapper {
    position: relative;
    width: 65%;
    margin: 30rpx auto 30rpx;
}

.invite-card {
    display: block;
    width: auto;
}


.invite-dialog .btn-wrapper {
    position: absolute;
    bottom: 0;
    left: 0;
    right: 0;
    z-index: 10;
    width: 100%;
    margin: 0 auto;
    display: flex;
    align-items: center;
    justify-content: center;
}

.invite-dialog .btn-wrapper .btn {
    position: relative;
    flex: 1;
    display: flex;
    align-items: center;
    justify-content: center;
    background-color: #fff;
    border-radius: 0;
    height: 98rpx;
    padding: 0;
    overflow: hidden;
    font-size: 28rpx;
    line-height: 98rpx;
    color: #3c4856;
}

.invite-dialog .btn-wrapper .btn:not(:last-child) {
    border-right: 1px solid #ededed;
}

.invite-dialog .btn-wrapper .icon-save {
    display: block;
    margin-right: 10rpx;
    width: 48rpx;
    height: 48rpx;
    background: transparent url(data:image/png;base64,) center center no-repeat;
    background-size: 48rpx;
}

.invite-dialog .btn-wrapper .icon-fri {
    display: block;
    margin-right: 10rpx;
    width: 48rpx;
    height: 48rpx;
    background: transparent url(data:image/png;base64,) center center no-repeat;
    background-size: 48rpx;
}

.invite-dialog .btn-wrapper .icon-circle {
    display: block;
    margin-right: 10rpx;
    width: 48rpx;
    height: 48rpx;
    background: transparent url(data:image/png;base64,) center center no-repeat;
    background-size: 48rpx;
}









